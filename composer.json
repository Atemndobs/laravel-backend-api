{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "ext-dom": "*",
        "ext-libxml": "*",
        "aerni/laravel-spotify": "^2.0",
        "backpack/crud": "^5.0",
        "backpack/logmanager": "^4.0",
        "backpack/permissionmanager": "^6.0",
        "backpack/settings": "^3.0",
        "beyondcode/laravel-websockets": "^1.13",
        "dbfx/laravel-strapi": "*",
        "fabpot/goutte": "^4.0",
        "filament/filament": "^2.0",
        "fruitcake/laravel-cors": "^2.0.5",
        "guzzlehttp/guzzle": "^7.2",
        "http-interop/http-factory-guzzle": "^1.2",
        "jwilsson/spotify-web-api-php": "^5.0",
        "laravel/framework": "^9.0",
        "laravel/sanctum": "^2.14",
        "laravel/scout": "^9.4",
        "laravel/tinker": "^2.7",
        "league/flysystem-ftp": "^3.0",
        "league/flysystem-sftp-v3": "^3.0",
        "maatwebsite/excel": "^3.1",
        "mateusjunges/laravel-kafka": "^1.6",
        "meilisearch/meilisearch-php": "^0.23.1",
        "php-decimal/php-decimal": "^1.1",
        "prettus/l5-repository": "^2.8",
        "psr/simple-cache": "^1.0",
        "spatie/crawler": "^7.1",
        "spatie/laravel-permission": "^5.5",
        "spatie/laravel-route-discovery": "^1.0",
        "stancl/tenancy": "^3.5",
        "tailflow/laravel-orion": "^2.11",
        "vladimir-yuldashev/laravel-queue-rabbitmq": "^12.0"
    },
    "require-dev": {
        "backpack/generators": "^3.1",
        "fakerphp/faker": "^1.9.1",
        "laravel-shift/blueprint": "^2.3",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "reliese/laravel": "^1.1",
        "spatie/laravel-ignition": "^1.0",
        "spatie/laravel-ray": "^1.29"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
    }
}
